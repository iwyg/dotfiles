{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "settings": {
        "ecmascript": 6,
        "jsx": true
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "strict": 1,
        "quotes": 0,
        "no-unused-vars": 1,
        "camelcase": 0,
        "no-underscore-dangle": 0,
		"semi": [2, "always"]
    }
}
